{
  "name": "my_indiaa_e-com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "tsc -p tsconfig-build.json --watch",
    "start": "node dist/main/index.js",
    "dev": "yarn run build && nodemon -L --watch ./dist ./dist/main/index.js",
    "start:dev": "concurrently --kill-others-on-fail \"yarn run build:watch\" \"yarn run dev\"",
    "start:ts": "ts-node-dev --respawn --transpile-only --exit-child ./src/main/index.ts",
    "format:fix": "prettier --write \"**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json}\"",
    "lint": "eslint --ignore-path .gitignore --ext .ts --fix",
    "lint:check": "eslint --max-warnings 0 --config .eslintrc .",
    "lint:fix": "eslint --max-warnings 0 --config .eslintrc . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "jose": "^5.4.1",
    "module-alias": "^2.2.3",
    "mongoose": "^8.4.3",
    "zod": "^3.23.8"
  },
  "_moduleAliases": {
    "@myIndiaa/controllers": "dist/controllers",
    "@myIndiaa/routes": "dist/routes",
    "@myIndiaa/utils": "dist/utils",
    "@myIndiaa/lib": "dist/lib",
    "@myIndiaa/middlewares": "dist/middlewares",
    "@myIndiaa/main": "dist/main"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2"
  }
}
